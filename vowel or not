//{ Driver Code Starts
#include<bits/stdc++.h> 
using namespace std; 

// } Driver Code Ends
class Solution{   
public:
    string isVowel(char c){
        // code here
 return (c == 'u' || c == 'e' || c == 'o' || c == 'a' || c == 'i' ||
                c == 'U' || c == 'E' || c == 'O' || c == 'A' || c == 'I') ? "YES" : "NO";    }
};

//{ Driver Code Starts.
int main() 
{ 
    int t;
    cin>>t;
    while(t--)
    {
        char c;
        cin >> c;
        Solution ob;
        cout << ob.isVowel(c) << endl;
    }
    return 0; 
} 
// } Driver Code Ends
