#include <iostream>
 
int main() { 
     int n;
     if (n > 5)
        {
            std::cout << "Greater than 5 ";
        }
    else if (n < 5)
    { 
        std::cout << "Less than 5";
    }
 else 
 {
    std::cout << "Equal to 5";
 }
 
 return 0;
}

// another  one 

//  Driver Code Ends
class Solution{   
public:
    string compareFive(int n){
        if (n > 5)
        {
            cout << "Greater than 5 ";
        }
    else if (n < 5)
    { 
        cout << "Less than 5";
    }
 else 
 {
     cout << "Equal to 5";
 }
    }

};
// driver code ends

